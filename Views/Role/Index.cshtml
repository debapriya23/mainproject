@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>
@{
    ViewData["Title"] = "Index";
}

<div><a asp-action="Create">Back to Create</a></div>


<h1>Index</h1>
<table class="table table-striped table-bordered">
    <tr>
        <td>Id</td>
        <td>Name</td>
    </tr>

    @foreach (var role in Model)
    {
        <tr>
            <td>@role.Id</td>
            <td>@role.Name</td>
        </tr>
    }
</table>

